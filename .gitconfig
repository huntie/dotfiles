[diff]
    tool = diffmerge
    compactionHeuristic = true
[push]
    default = simple
[apply]
    whitespace = nowarn
[commit]
    gpgsign = true
[alias]
    s = status
    cm = commit
    d = diff
    ch = checkout
    a = add
    log = log --pretty=oneline
[core]
    excludesfile = ~/.gitignore
